{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"keywords": [],
	"license": "ISC",
	"author": "",
	"main": "index.js",
	"scripts": {
		"build": "npm run clean && npm run build:js",
		"build:js": "sucrase ./src -d ./dist --transforms typescript,imports",
		"clean": "rimraf ./dist",
		"lint": "npm run lint:eslint && npm run lint:prettier",
		"lint:eslint": "eslint . --cache --fix --cache-location ./temp/eslint-cache --ignore-path ./.gitignore --config ./config/.eslintrc.js",
		"lint:eslint:commit": "eslint . --cache --fix --cache-location ./temp/eslint-cache --ignore-path ./.gitignore --config ./config/.eslintrc.commit.js",
		"lint:eslint:debug": "cross-env TIMING=1 && eslint .",
		"lint:prettier": "prettier . --write --config ./config/.prettierrc.js --ignore-path ./.gitignore",
		"lint:prettier:debug": "prettier . --write --config ./config/.prettierc.js --ignore-path ./.gitignore",
		"nuke": "cli-confirm \"Do you really want to blow **** up?\" && npm run nuke:confirmed",
		"nuke:confirmed": "npm install -g npm@latest && npm audit fix && rimraf node_modules && rimraf package-lock.json && rimraf ./temp && npm cache clean --force && npm i",
		"pre-commit": "lint-staged -c ./config/lint-staged.config.js",
		"prepare": "husky install",
		"start": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only --transpiler sucrase/ts-node-plugin ./src/index.ts",
		"start:production": "cross-env NODE_ENV=production node dist/index",
		"test": "cross-env NODE_ENV=test jest --clearCache && jest --config ./config/jest.config.ts"
	},
	"prettier": "./config/.prettierrc.js",
	"dependencies": {
		"@lawlzer/helpers": "^3.2.9",
		"@oauth-everything/passport-discord": "^1.0.2",
		"axios": "^1.1.3",
		"bcryptjs": "^2.4.3",
		"better-module-alias": "^1.1.0",
		"connect-mongo": "^4.6.0",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-session": "^1.17.3",
		"express-slow-down": "^1.5.0",
		"express-zod-api": "^8.3.4",
		"fs-extra": "^10.1.0",
		"mongoose-encryption": "^2.1.2",
		"morgan": "^1.10.0",
		"ms": "^2.1.3",
		"passport": "^0.6.0",
		"passport-github2": "^0.1.12",
		"passport-google-oauth": "^2.0.0",
		"passport-local": "^1.0.0",
		"validatorjs": "^3.22.1",
		"zod": "^3.19.1"
	},
	"devDependencies": {
		"@sucrase/jest-plugin": "^3.0.0",
		"@types/bcryptjs": "^2.4.2",
		"@types/cookie-parser": "^1.4.3",
		"@types/cookie-session": "^2.0.44",
		"@types/cors": "^2.8.12",
		"@types/express-session": "^1.17.5",
		"@types/express-slow-down": "^1.3.2",
		"@types/fs-extra": "^9.0.13",
		"@types/jest": "^29.2.2",
		"@types/module-alias": "^2.0.1",
		"@types/moment": "^2.13.0",
		"@types/mongoose": "^5.11.97",
		"@types/morgan": "^1.9.3",
		"@types/ms": "^0.7.31",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.11.9",
		"@types/passport": "^1.0.11",
		"@types/passport-discord": "^0.1.6",
		"@types/passport-github2": "^1.2.5",
		"@types/passport-google-oauth": "^1.0.42",
		"@types/passport-local": "^1.0.34",
		"@types/supertest": "^2.0.12",
		"@types/validatorjs": "^3.15.0",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"@typescript-eslint/parser": "^5.42.1",
		"cli-confirm": "^1.0.1",
		"cross-env": "^7.0.3",
		"esbuild": "^0.15.13",
		"eslint": "^8.27.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^8.0.2",
		"jest": "^29.3.1",
		"lint-staged": "^13.0.3",
		"mongoose": "^6.7.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"prettier-plugin-packagejson": "^2.3.0",
		"regenerator-runtime": "^0.13.10",
		"rimraf": "^3.0.2",
		"run-p": "^0.0.0",
		"sucrase": "^3.28.0",
		"supertest": "^6.3.1",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.1.0",
		"typescript": "^4.8.4"
	},
	"_moduleAliases": {
		"~": "./src"
	}
}
